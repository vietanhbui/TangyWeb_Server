<div class="row">
    <h4 class="text-primary p-lg-4 pt-2 col-12">Grand Child Component</h4>
    <p>This is a message - @MessageForGrandChild</p>
    <p>This is a message - @LuckyNumberForGrandChild</p>
</div>

@code {
    [CascadingParameter(Name = "message")]
    public string MessageForGrandChild { get; set; }
    [CascadingParameter(Name = "luckyNumber")]
    public string LuckyNumberForGrandChild { get; set; }
}
